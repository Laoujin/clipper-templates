{
	"schemaVersion": "0.1.0",
	"name": "Goodreads",
	"behavior": "create",
	"noteContentFormat": "\n# Summary\n{{selector:.BookPageMetadataSection__description|replace:\"/Show more$/\":\"\"}}\n\n[Open Goodreads]({{url}})\n\n{{selector:.BookCover__image img?src|first|image:\"Cover\"}}",
	"properties": [
		{
			"name": "categories",
			"value": "[[Books]]",
			"type": "multitext"
		},
		{
			"name": "title",
			"value": "{{selector:.BookPageTitleSection__title h1}}",
			"type": "text"
		},
		{
			"name": "series",
			"value": "{{selector:.BookPageTitleSection__title>h3|split:\\\"#\\\"|first|trim|wikilink}}",
			"type": "text"
		},
		{
			"name": "serierNr",
			"value": "{{selector:.BookPageTitleSection__title>h3|split:\\\"#\\\"|last}}",
			"type": "number"
		},
		{
			"name": "author",
			"value": "{{selector:.ContributorLink__name|unique|wikilink|join}}",
			"type": "multitext"
		},
		{
			"name": "genre",
			"value": "{{selector:.BookPageMetadataSection__genres .BookPageMetadataSection__genreButton|wikilink|join}}",
			"type": "multitext"
		},
		{
			"name": "pages",
			"value": "{{selector:p[data-testid='pagesFormat'|split:\\\", \\\"|slice:0|split:\\\" \\\"|slice:0}}",
			"type": "number"
		},
		{
			"name": "year",
			"value": "{{selector:p[data-testid='publicationInfo']|split:\\\"First published \\\"|slice:1|date:\\\"YYYY\\\"}}",
			"type": "number"
		},
		{
			"name": "scoreGr",
			"value": "{{selector:.RatingStatistics__rating|first|round:2}}",
			"type": "number"
		},
		{
			"name": "ratingCount",
			"value": "{{selector:[data-testid=ratingsCount]|first}}",
			"type": "text"
		},
		{
			"name": "rating",
			"value": "{{selector:.BookRatingStars>span?aria-label|first|split:\\\" \\\"|nth:2|first|replace:\\\"0\\\",\\\"\\\"}}",
			"type": "text"
		},
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "tags",
			"value": "books, to-read",
			"type": "multitext"
		},
		{
			"name": "last",
			"value": "",
			"type": "date"
		}
	],
	"triggers": [
		"https://www.goodreads.com/book/"
	],
	"noteNameFormat": "{{selector:.BookPageTitleSection__title h1}}",
	"path": "References"
}